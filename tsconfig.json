{
  "include": ["src"],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@UI": ["src/components/UI"],
      "@UI/*": ["src/components/UI/*"],
      "@common": ["src/components/common"],
      "@common/*": ["src/components/common/*"],
      "@styles": ["src/styles"],
      "@styles/*": ["src/styles/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"]
    },
    "jsx": "react-jsx"
  }
}
